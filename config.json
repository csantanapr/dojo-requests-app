{
	"id": "requestsApp",
	"loaderConfig": {
		"paths": {
			"requestsApp": "../requestsApp"
		}
	},
	"dependencies": [
		"dojo/store/Observable",
		"dojox/mobile/compat",
		"dojox/mobile/Heading",
		"dojox/mobile/ScrollableView",
		"dojox/mobile/View",
		"dojox/mobile/ToolBarButton",
		"dojo/store/Memory",
		"dojox/mobile/ExpandingTextArea",
		"dojox/mobile/ComboBox",
		"dojox/mobile/RoundRectStoreList",
		"dojox/mobile/EdgeToEdgeCategory",
		"dojox/mobile/Opener",
		"dojox/mobile/ToolBarButton",
		"dojox/mobile/DatePicker",
		"dojox/mobile/Button"
	],
	"modules": [
	],
	"controllers": [
		"dojox/app/controllers/Load",
		"dojox/app/controllers/Transition",
		"dojox/app/controllers/Layout"
	],
	"nls": "requestsApp/nls/requests",
	"defaultTransition": "slide",
	"defaultView": "requestList",
	"stores": {
		"requestsListStore":{
			"type": "dojo/store/Memory",
			"observable": true,
			"params": {
				"data": "requestsApp.requestsListData"
      		}
  		},
		"requestTypeStore":{
			"type": "dojo/store/Memory",
			"params": {
				"data": "requestsApp.requestsTypeData"
      		}
  		},
		"requestStatusStore":{
			"type": "dojo/store/Memory",
			"params": {
				"data": "requestsApp.requestsStatusData"
      		}
  		},
		"requestPriorityStore":{
			"type": "dojo/store/Memory",
			"params": {
				"data": "requestsApp.requestsPriorityData"
      		}
  		},
		"requestUnitTypeStore":{
			"type": "dojo/store/Memory",
			"params": {
				"data": "requestsApp.requestsUnitTypeData"
      		}
  		}
	},
	"views": {
		"requestList":{
			"controller" : "requestsApp/views/list.js",
			"template": "requestsApp/views/list.html",
			"nls": "requestsApp/nls/list",
			"dependencies":["dojox/mobile/TextBox"],
			"has": {
				"!phone": {
					"constraint": "left"
				}
			}
		},
		"requestItemDetails":{
			"controller" : "requestsApp/views/detail.js",
			"template": "requestsApp/views/detail.html",
			"nls": "requestsApp/nls/requestsDetail"
		}
	},
	"has": {
		"html5history": {
			"controllers": [
				"dojox/app/controllers/History"
			]
		},
		"!html5history": {
			"controllers": [
				"dojox/app/controllers/HistoryHash"
			]
		}
	}
}
